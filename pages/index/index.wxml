<!-- 当前订单模块 -->
<view class="title padding10 leftpadding">
  当前订单
</view>
<view class="menu">
  <navigator
  url="/pages/newselllist/newselllist" 
  hover-class="navigator-hover" 
  open-type="navigate">
    <text class="item_img_special iconfont icon-dingdandaifukuan"> 
    </text>
    <view class="item_name">
      新的销售订单
    </view>
    <view class="item_num">
      {{newsellnum}}
