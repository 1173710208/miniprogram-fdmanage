  快捷操作
</view>
<view class="menu">
  <navigator
  url="/pages/books/bookslist" 
  hover-class="navigator-hover" 
  open-type="navigate"
  bindtap='scanCode'>
    <text class="item_img_convenience iconfont icon-saoyisao"> 
    </text>
    <view class="item_name"> 
      扫码新增图书
    </view>
  </navigator>

  <navigator
  url="/pages/customer/newcustomer" 
  hover-class="navigator-hover" 
  open-type="navigate">
    <text class="item_img_convenience iconfont icon-biaodankongjianlianxiren"> 
    </text>
    <view class="item_name">
      新增会员
    </view>
  </navigator>
</view>

